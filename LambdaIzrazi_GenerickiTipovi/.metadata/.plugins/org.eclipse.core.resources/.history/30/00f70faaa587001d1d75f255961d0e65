package milosbogdanovic;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		ObrniZnak oz = (str) -> {  
			String rezultat = "";
			
			for(int i = str.length() - 1; i >= 0; i--)
				rezultat += str.charAt(i);
			return rezultat;
		};
		
		System.out.println("12345" + oz.obrni(" 12345"));

	}

}
