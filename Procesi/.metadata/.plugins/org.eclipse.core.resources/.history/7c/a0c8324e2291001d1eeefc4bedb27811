package milosbogdanovic;

public class A extends Thread{
	
	public void run() {
		
		int i = 1;
		while (i <= 10) {
			System.out.println("i: " + i);
			
			// sleep(vrednostUMilisekundama) - 
			
			try {
				Thread.sleep(5000);
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
			i++;
		}
	}

}
